module intri-annotations {

  namespace "intri:intri-annotations";
  prefix "intri-annotations";
  organization "
    Intrising Inc.";
  contact "
    intrising@intrising.com.tw";
  revision 2022-06-24 {
    description "Build date.";
  }

  grouping Http {
    container Rules {
      list Rules_Entry {
        uses HttpRule;
      }
    }
    leaf FullyDecodeReservedExpansion {
      type boolean;
    }
  }
  grouping HttpRule {
    leaf Selector {
      type string;
    }
    choice Pattern {
      leaf Get {
        type string;
      }
      leaf Put {
        type string;
      }
      leaf Post {
        type string;
      }
      leaf Delete {
        type string;
      }
      leaf Patch {
        type string;
      }
      container Custom {
        uses CustomHttpPattern;
      }
    }
    leaf Body {
      type string;
    }
    leaf ResponseBody {
      type string;
    }
    container AdditionalBindings {
      list AdditionalBindings_Entry {
        uses HttpRule;
      }
    }
  }
  grouping CustomHttpPattern {
    leaf Kind {
      type string;
    }
    leaf Path {
      type string;
    }
  }
}
