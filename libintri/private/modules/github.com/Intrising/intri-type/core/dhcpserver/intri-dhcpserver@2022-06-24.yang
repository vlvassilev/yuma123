module intri-dhcpserver {

  namespace "intri:intri-dhcpserver";
  prefix "intri-dhcpserver";
  organization "
    Intrising Inc.";
  contact "
    intrising@intrising.com.tw";
  revision 2022-06-24 {
    description "Build date.";
  }

  import intri-device {
    prefix intri-device;
  }
  import intri-empty {
    prefix intri-empty;
  }
  typedef PoolTypeOptions {
    type enumeration {
      enum POOL_TYPE_BASIC {
        description "";
        value 0;
      }
      enum POOL_TYPE_MAC_BASED {
        description "";
        value 1;
      }
      enum POOL_TYPE_PORT_BASED {
        description "";
        value 2;
      }
    }
  }
  typedef InterfaceType {
    type enumeration {
      enum INTERFACE_TYPE_PORT {
        description "[Port] Select a port as the interface";
        value 0;
      }
      enum INTERFACE_TYPE_VLAN {
        description "[VLAN] Select a VLAN as the interface";
        value 1;
      }
    }
  }
  grouping Config {
    container V4 {
      uses V4Config;
    }
  }
  grouping V4Config {
    container Sys {
      uses System;
    }
    container Pool {
      uses Pool;
    }
  }
  grouping System {
    leaf Enabled {
      type boolean;
    }
  }
  grouping Pool {
    container List {
      list List_Entry {
        uses PoolEntry;
      }
    }
  }
  grouping PoolEntry {
    leaf Name {
      type string;
    }
    container Interface {
      uses intri-device:InterfaceIdentify;
    }
    leaf EntryType {
      type PoolTypeOptions;
    }
    container Basic {
      uses Basic;
    }
    container MACBased {
      uses MACBased;
    }
    container PortBased {
      uses PortBased;
    }
    leaf ID {
      type string;
    }
  }
  grouping Basic {
    leaf RangeLow {
      type string;
    }
    leaf RangeHigh {
      type string;
    }
    leaf Netmask {
      type string;
    }
    leaf Gateway {
      type string;
    }
    leaf PrimaryDNS {
      type string;
    }
    leaf SecondaryDNS {
      type string;
    }
    leaf LeaseTime {
      type int32;
    }
  }
  grouping MACBased {
    leaf MACAddress {
      type string;
    }
    leaf DesiredIP {
      type string;
    }
  }
  grouping PortBased {
    leaf PortNo {
      type int32;
    }
    leaf Ignore {
      type boolean;
    }
    leaf DesiredIP {
      type string;
    }
  }
  grouping StatusEntry {
    leaf Name {
      type string;
    }
    container Interface {
      uses intri-device:InterfaceIdentify;
    }
    leaf PortNo {
      type int32;
    }
    leaf MACAddress {
      type string;
    }
    leaf IPAddress {
      type string;
    }
    leaf AvailableLeaseTime {
      type int32;
    }
  }
  grouping Status {
    container List {
      list List_Entry {
        uses StatusEntry;
      }
    }
  }
  rpc intri-dhcpserver-DHCPServer-GetV4Config {
    output {
      uses V4Config;
    }
  }
  rpc intri-dhcpserver-DHCPServer-SetV4System {
    input {
      uses System;
    }
  }
  rpc intri-dhcpserver-DHCPServer-AddV4Basic {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-UpdateV4Basic {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-DeleteV4Basic {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-GetV4Status {
    output {
      uses Status;
    }
  }
  rpc intri-dhcpserver-DHCPServer-GetV4MACBased {
    output {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-AddV4MACBased {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-DeleteV4MACBased {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-UpdateV4MACBased {
    input {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-GetV4PortBased {
    output {
      uses Pool;
    }
  }
  rpc intri-dhcpserver-DHCPServer-UpdateV4PortBased {
    input {
      uses Pool;
    }
  }
}
